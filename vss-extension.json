{
  "manifestVersion": 1,
  "id": "CookbookVersion",
  "name": "Cookbook Version",
  "version": "1.0.0",
  "publisher": "Raghureddy45",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A build task that reads the version from the metadata.rb file and makes it available as build variables.",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Chef",
    "Version"
  ],
  "icons": {
    "default": "images/cookbook.jpg"
  },
  "files": [
    {
      "path": "cookbook-version"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/Raghureddy45/cookbook-version-check"
    },
    "support": {
      "uri": "https://github.com/Raghureddy45/cookbook-version-check/Issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Raghureddy45/cookbook-version-check"
  },
  "galleryFlags": [
    "Public"
  ],
  "contributions": [
    {
      "id": "cookbook-version-reader-build-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "cookbook-version-check"
      }
    }
  ]
}
